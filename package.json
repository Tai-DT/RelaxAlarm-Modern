{
  "name": "relaxalarm-modern",
  "version": "1.0.0",
  "description": "Modern relaxation and sleep app with natural themes and smart features",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "^4.5.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tamagui/animations-react-native": "^1.103.3",
    "@tamagui/config": "^1.103.3",
    "@tamagui/core": "^1.103.3",
    "@tamagui/theme": "^1.103.3",
    "expo": "~51.0.0",
    "expo-app-loading": "^2.1.1",
    "expo-av": "~14.0.5",
    "expo-constants": "~16.0.1",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.5",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.12.9",
    "expo-linear-gradient": "~13.0.2",
    "expo-notifications": "~0.28.7",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-task-manager": "~11.8.1",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "^0.74.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "detox": "^20.19.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.2",
    "lint-staged": "^15.2.2",
    "metro": "^0.80.6",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "keywords": [
    "react-native",
    "expo",
    "relaxation",
    "sleep",
    "alarm",
    "meditation",
    "nature-sounds",
    "wellness",
    "mindfulness",
    "typescript",
    "material-design"
  ],
  "author": {
    "name": "Tai-DT",
    "email": "tai@relaxalarm.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tai-DT/RelaxAlarm-Modern.git"
  },
  "bugs": {
    "url": "https://github.com/Tai-DT/RelaxAlarm-Modern/issues"
  },
  "homepage": "https://github.com/Tai-DT/RelaxAlarm-Modern#readme",
  "private": false,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ]
  }
}